{
  "name": "shpd-bwc-app",
  "version": "0.0.1",
  "private": true,
  "description": "SHPD BodyWorn Camera Tagger",
  "homepage": "https://github.com/BTBento/SHPD-BWC-App#readme",
  "bugs": {
    "url": "https://github.com/BTBento/SHPD-BWC-App/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BTBento/SHPD-BWC-App.git"
  },
  "main": "./dist/main.js",
  "scripts": {
    "dev": "npm-run-all -p dev:react start:backend electron:serve",
    "start:backend": "conda run -n py310 --cwd ./src uvicorn backend.main:app --host 127.0.0.1 --port 8000",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/electron.webpack.ts --mode=development && npm run start:electron",
    "dev:react": "cross-env NODE_ENV=development webpack serve --config webpack/react.webpack.ts --mode=development",
    "electron:serve": "wait-on http-get://localhost:4000/ && npm run dev:electron",
    "start:electron": "electron .",
    "build": "npm-run-all build:electron build:react",
    "build:run": "npm-run-all build start:electron",
    "build:electron": "webpack --config webpack/electron.webpack.ts --mode=production",
    "build:react": "webpack --config webpack/react.webpack.ts --mode=production",
    "package": "npm-run-all build package:dist",
    "package:dist": "electron-builder --dir"
  },
  "keywords": [],
  "license": "ISC",
  "author": "Alex Jen, Alex Brown, HelloSoftware",
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.9.1",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.0",
    "trash": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^36.1.0",
    "electron-builder": "^26.0.12",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "wait-on": "^8.0.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "build": {
    "appId": "cs180.shpd.app",
    "productName": "SHPD BodyWorn Camera Tagger",
    "mac": {
      "category": "public.app-category.video"
    },
    "directories": {
      "output": "packages"
    },
    "files": [
      "package.json",
      "dist/**"
    ]
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.13.0"
  }
}
